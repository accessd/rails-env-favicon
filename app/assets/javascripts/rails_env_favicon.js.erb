<% unless Rails.env.production? %>
    <%= require_asset "tinycon.min.js" %>
    Tinycon.setOptions({
        width: 8,
        height: 9,
        font: '10px arial',
        colour: '<%= RailsEnvFavicon.text_color %>',
        background: '<%= RailsEnvFavicon.background_color %>',
        fallback: true
    });

    Tinycon.setBubble("<%= RailsEnvFavicon.env_letter %>");
<% end %>
